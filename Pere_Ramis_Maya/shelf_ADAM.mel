me)\n{\n\tint $val;\n\tint $min;\n\tint $max;\n\tint $mid;\n\tfloat $keyVals[];\n\tglobal string $gPlayBackSlider;\n    float $range[]=`timeControl -q -rangeArray $gPlayBackSlider`;\n\n\t$selected = `ls -sl`;\n//\t$hier\t= `select -r -hi $selected`;\n\t\n\t$keys = `selectKey -time ($range[0]+\":\"+$range[1])`;\n\n\t$keyVals = `keyframe -q -sl`;\n\n\t$min = $keyVals[0]; $max = $keyVals[0];\n\n\tfor ($val in $keyVals)\n\t\t{\n\t\tif ($val < $min) $min = $val;\n\t\tif ($val > $max) $max = $val;\n\t\t}\n\n\t//$mid = (($min + $max) / 2);\n\t$mid = (($max - $min) / 2 + $min);\n\t\n\tprint (\"Start frame : \" + $min + \"\\n\");\n\tprint (\"End frame : \" + $max + \"\\n\");\n\tprint (\"Mid Point : \" + $mid + \"\\n\");\n\tif ($min != $max){\t\t\n\tswitch ($pivot){\n\t\tcase 0:\n\t\t\t\tscaleKey -scaleSpecifiedKeys 1 -timeScale -1 -timePivot $mid;\n\t\t\t\tprint (\"Reversing keys for selected objects and below at frame : \" + $mid + \"\\n\");\n\t\t\t\tbreak;\n\t\tcase 1:\n\t\t\t\tscaleKey -scaleSpecifiedKeys 1 -timeScale -1 -timePivot $min;\n\t\t\t\tprint (\"Reversing keys for selected objects and below at frame : \" + $min + \"\\n\");\n\t\t\t\tbreak;\n\t\tcase 2:\n\t\t\t\tscaleKey -scaleSpecifiedKeys 1 -timeScale -1 -timePivot $max;\n\t\t\t\tprint (\"Reversing keys for selected objects and below at frame : \" + $max + \"\\n\");\n\t\t\t\tbreak;}\n\t}else { print \"You must select any key you want to set reverse\";}\n\t\n\n\tselect -r $selected;\n}\n\nglobal proc setReverseAnimOptionVars()\n{\n\tif (`optionVar -ex bgReversePivot` == 0)\n\t\toptionVar -intValue bgReversePivot 0;   // 0=mid : 1:Start : 2:End : 3:Frame\n\tif (`optionVar -ex bgReverseFrameNumber` == 0)\n\t\toptionVar -intValue bgReverseFrameNumber 0;\n}\n\nglobal proc deleteReverseAnimOptionVars()\n{\n\toptionVar -rm bgReversePivot;\n\toptionVar -rm bgReverseFrameNumber;\n}\n\nglobal proc c_ReverseAnim (){\n\n    $RgChangehand = `floatSliderGrp -q -value RgikfkChangehand`;\n    select \"Adam_ac_Rg_IkFkChangeArm\"